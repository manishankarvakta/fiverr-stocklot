{
  "custom_reports": [
    {
      "name": "Review System Performance",
      "description": "Comprehensive review system analytics and conversion funnel",
      "dimensions": [
        "eventName",
        "customEvent:review_direction",
        "customEvent:review_rating",
        "userType",
        "deviceCategory"
      ],
      "metrics": [
        "eventCount",
        "totalUsers",
        "conversions",
        "conversionRate",
        "eventValue"
      ],
      "filters": [
        {
          "fieldName": "eventName",
          "operation": "CONTAINS",
          "value": "review"
        }
      ],
      "orderBys": [
        {
          "metric": "eventCount",
          "desc": true
        }
      ]
    },
    {
      "name": "Livestock Marketplace Funnel",
      "description": "Complete user journey from search to purchase",
      "dimensions": [
        "eventName",
        "customEvent:livestock_category",
        "customEvent:price_range",
        "country",
        "region"
      ],
      "metrics": [
        "eventCount",
        "totalUsers",
        "newUsers",
        "sessions",
        "conversions",
        "totalRevenue"
      ],
      "filters": [
        {
          "fieldName": "eventName",
          "operation": "IN_LIST",
          "values": [
            "livestock_search",
            "listing_viewed",
            "offer_made",
            "purchase"
          ]
        }
      ]
    },
    {
      "name": "Seller Performance Dashboard",
      "description": "Track seller engagement and review responses",
      "dimensions": [
        "customEvent:seller_id",
        "eventName",
        "customEvent:seller_rating",
        "date"
      ],
      "metrics": [
        "eventCount",
        "totalUsers",
        "averageSessionDuration",
        "eventValue"
      ],
      "filters": [
        {
          "fieldName": "eventName",
          "operation": "IN_LIST",
          "values": [
            "review_reply_created",
            "seller_profile_viewed",
            "listing_viewed"
          ]
        }
      ]
    },
    {
      "name": "User Segmentation Analysis",
      "description": "Analyze user behavior by type and engagement level",
      "dimensions": [
        "customEvent:user_type",
        "customEvent:kyc_level",
        "customEvent:registration_method",
        "newVsReturning"
      ],
      "metrics": [
        "totalUsers",
        "newUsers",
        "sessions",
        "screenPageViews",
        "averageSessionDuration",
        "conversions",
        "totalRevenue"
      ],
      "segments": [
        "high_value_buyers",
        "active_reviewers",
        "top_sellers"
      ]
    },
    {
      "name": "Geographic Performance - South Africa",
      "description": "Performance analysis by South African provinces",
      "dimensions": [
        "region",
        "city",
        "customEvent:location_province",
        "customEvent:livestock_category"
      ],
      "metrics": [
        "totalUsers",
        "sessions",
        "conversions",
        "totalRevenue",
        "averageSessionDuration"
      ],
      "filters": [
        {
          "fieldName": "country",
          "operation": "EXACT",
          "value": "South Africa"
        }
      ]
    }
  ],
  "conversion_goals": [
    {
      "name": "Review Submission",
      "event_name": "review_created",
      "value": 5,
      "currency": "ZAR",
      "conditions": [
        {
          "parameter": "review_rating",
          "operation": "GREATER_THAN_OR_EQUAL",
          "value": 1
        }
      ]
    },
    {
      "name": "User Registration",
      "event_name": "sign_up",
      "value": 25,
      "currency": "ZAR",
      "conditions": [
        {
          "parameter": "registration_method",
          "operation": "IS_NOT_NULL"
        }
      ]
    },
    {
      "name": "Livestock Purchase",
      "event_name": "purchase",
      "value": "dynamic",
      "currency": "ZAR",
      "conditions": [
        {
          "parameter": "value",
          "operation": "GREATER_THAN",
          "value": 0
        }
      ]
    },
    {
      "name": "High Value Purchase",
      "event_name": "purchase",
      "value": "dynamic",
      "currency": "ZAR",
      "conditions": [
        {
          "parameter": "value",
          "operation": "GREATER_THAN",
          "value": 10000
        }
      ]
    },
    {
      "name": "Premium Seller Signup",
      "event_name": "subscription_changed",
      "value": 100,
      "currency": "ZAR",
      "conditions": [
        {
          "parameter": "subscription_tier",
          "operation": "EXACT",
          "value": "premium"
        }
      ]
    },
    {
      "name": "KYC Completion",
      "event_name": "kyc_completed",
      "value": 15,
      "currency": "ZAR",
      "conditions": [
        {
          "parameter": "kyc_level",
          "operation": "GREATER_THAN_OR_EQUAL",
          "value": 2
        }
      ]
    }
  ],
  "custom_dimensions": [
    {
      "parameter_name": "user_type",
      "display_name": "User Type",
      "description": "Buyer, Seller, or Both",
      "scope": "USER"
    },
    {
      "parameter_name": "kyc_level",
      "display_name": "KYC Level", 
      "description": "User verification level (0-3)",
      "scope": "USER"
    },
    {
      "parameter_name": "livestock_category",
      "display_name": "Livestock Category",
      "description": "Type of livestock (Cattle, Sheep, Goats, etc.)",
      "scope": "EVENT"
    },
    {
      "parameter_name": "review_direction",
      "display_name": "Review Direction",
      "description": "BUYER_ON_SELLER or SELLER_ON_BUYER",
      "scope": "EVENT"
    },
    {
      "parameter_name": "review_rating",
      "display_name": "Review Rating",
      "description": "Star rating (1-5)",
      "scope": "EVENT"
    },
    {
      "parameter_name": "seller_rating",
      "display_name": "Seller Rating",
      "description": "Average seller rating",
      "scope": "EVENT"
    },
    {
      "parameter_name": "location_province",
      "display_name": "South African Province",
      "description": "Province where user is located",
      "scope": "USER"
    },
    {
      "parameter_name": "registration_method",
      "display_name": "Registration Method",
      "description": "How user registered (email, google, etc.)",
      "scope": "USER"
    }
  ],
  "audiences": [
    {
      "name": "High Value Buyers",
      "description": "Users with high purchase value and frequency",
      "conditions": [
        {
          "dimension": "totalRevenue",
          "operator": "GREATER_THAN",
          "value": 50000
        },
        {
          "dimension": "eventCount",
          "operator": "GREATER_THAN_OR_EQUAL",
          "value": 3,
          "event": "purchase"
        }
      ]
    },
    {
      "name": "Active Reviewers",
      "description": "Users who frequently submit reviews",
      "conditions": [
        {
          "dimension": "eventCount",
          "operator": "GREATER_THAN_OR_EQUAL",
          "value": 5,
          "event": "review_created"
        }
      ]
    },
    {
      "name": "Top Sellers",
      "description": "Highly rated sellers with multiple sales",
      "conditions": [
        {
          "dimension": "customEvent:seller_rating",
          "operator": "GREATER_THAN_OR_EQUAL",
          "value": 4.5
        },
        {
          "dimension": "eventCount",
          "operator": "GREATER_THAN_OR_EQUAL",
          "value": 10,
          "event": "purchase"
        }
      ]
    },
    {
      "name": "At Risk Users",
      "description": "Users who haven't visited recently but have purchase history",
      "conditions": [
        {
          "dimension": "daysSinceLastSession",
          "operator": "GREATER_THAN",
          "value": 30
        },
        {
          "dimension": "eventCount",
          "operator": "GREATER_THAN_OR_EQUAL",
          "value": 1,
          "event": "purchase"
        }
      ]
    }
  ]
}